{
  "manifest_version": 3,
  "name": "Clash UI",
  "version": "1.0.0",
  "description": "clash ui for chrome",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "proxy"
  ]
}
